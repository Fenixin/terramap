        <Frame num="38" upperLeft="132,88"  name="Tree Top"    dir="Up"     variety="Small Right Slight Bulge B"  attachesTo="treeFloor; branchLeft"									 />
        <Frame num="39" upperLeft="154,88"  name="Tree Top"    dir="Left"   variety="Large B"                     attachesTo="treeFloor; treeRight"									 />

        <Frame num="40" upperLeft="0,110"   name="Tree Trunk"  dir="Up"     variety="Left Indent C"               attachesTo="treeFloor; treeCeiling"									 />
        <Frame num="41" upperLeft="22,110"  name="Tree Trunk"  dir="Up"     variety="Right Bulge C"               attachesTo="treeFloor; treeCeiling"									 />
        <Frame num="42" upperLeft="44,110"  name="Tree Trunk"  dir="Up"     variety="Left Bulge C"                attachesTo="treeFloor; treeCeiling"									 />
        <Frame num="43" upperLeft="66,110"  name="Tree Trunk"  dir="Up"     variety="Right Slight Bulge F"        attachesTo="treeFloor; treeCeiling; branchRight"				 />
        <Frame num="44" upperLeft="88,110"  name="Tree Branch" dir="Right"  variety="Plain C"                     attachesTo="treeRightBranch"											 />
        <Frame num="45" upperLeft="110,110" name="Tree Trunk"  dir="Up"     variety="Double Slight Bulge C"       attachesTo="treeFloor; treeCeiling; branchLeft; branchRight" />
        <Frame num="46" upperLeft="132,110" name="Tree Top"    dir="Up"     variety="Small Right Slight Bulge C"  attachesTo="treeFloor; branchLeft"									 />
        <Frame num="47" upperLeft="154,110" name="Tree Top"    dir="Left"   variety="Large C"                     attachesTo="treeFloor; treeRight"									 />

        <Frame num="48" upperLeft="0,132"   name="Tree Trunk"  dir="Left"   variety="Large A"                     attachesTo="treeFloor; treeCeiling; treeRight"					 />
        <Frame num="49" upperLeft="22,132"  name="Tree Base"   dir="Right"  variety="Plain A"                     attachesTo="treeFloor; treeLeft"										 />
        <Frame num="50" upperLeft="44,132"  name="Tree Base"   dir="Left"   variety="Plain A"                     attachesTo="treeFloor; treeRight"									 />
        <Frame num="51" upperLeft="66,132"  name="Tree Trunk"  dir="Right"  variety="Large A"                     attachesTo="treeFloor; treeCeiling; treeLeft"					 />
        <Frame num="52" upperLeft="88,132"  name="Tree Trunk"  dir="Center" variety="Large A"                     attachesTo="treeFloor; treeCeiling; treeLeft; treeRight"	 	 />
        <Frame num="53" upperLeft="132,132" name="Tree Top"    dir="Up"     variety="Small Double Slight Bulge A" attachesTo="treeFloor; branchRight"									 />
        <Frame num="54" upperLeft="154,132" name="Tree Top"    dir="Left"   variety="Huge"                        attachesTo="treeFloor; treeLeft; treeRight"						 />

